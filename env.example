# GeoBoundaries (public)
GEOB_BASE=https://www.geoboundaries.org/api/current

# Copernicus Climate Data Store (for ERA5)
CDS_URL=https://cds.climate.copernicus.eu/api
CDS_KEY=12345:your_cds_token

# API Security (optional)
API_KEY=dev-secret
AGGREGATION_FLOOR=5
